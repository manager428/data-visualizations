{{domainString}}

<button{{action "increaseData"}}>Increase</button>
<button{{action "decreaseData"}}>Decrease</button>

{{column-chart
    dimension=dimensions
    group=groups
    seriesData=content
    type='LAYERED'
    seriesMaxMin=2
    showMaxMin=true
    series=series
    colors=colors
    height=200
    xAxis=xAxis
    yAxis=yAxis
    onClick=(action onClick)
    showComparisonLine=true
    comparisonLine=comparisonLine
    currentInterval=currentInterval
    showCurrentIndicator=true
}}

{{row-chart
    dimension=dimensions
    group=groups
    series=series
    colors=colors
    height=800
    width=600
    xAxis=yAxis
    yAxis=xAxis
    onClick=(action onClick)
    currentInterval=currentInterval
    showCurrentIndicator=true
}}

{{outlet}}
