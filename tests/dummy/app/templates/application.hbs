{{domainString}}<br/>

<button{{action "increaseData"}}>Increase</button>
<button{{action "decreaseData"}}>Decrease</button>

{{column-chart
    dimension=dimensions
    group=groups
    seriesData=content
    type='LAYERED'
    seriesMaxMin=2
    showMaxMin=true
    series=series
    colors=colors
    height=200
    xAxis=xAxis
    yAxis=yAxis
    onClick=(action onClick)
    showComparisonLine=true
    comparisonLine=comparisonLine
    currentInterval=currentInterval
    showCurrentIndicator=false
}}

{{line-chart
    dimension=dimensions
    group=groups
    seriesData=content
    type='LAYERED'
    series=series
    colors=colors
    height=200
    xAxis=xAxis
    yAxis=yAxis
    onClick=(action onClick)
    showComparisonLine=true
    comparisonLine=comparisonLine
    currentInterval=currentInterval
    showCurrentIndicator=false
}}

{{input type='checkbox' checked=hideAxisLines}}Hide x tick lines<br/>
{{input type='checkbox' checked=showYTicks}}Show y ticks<br/>
{{input type='checkbox' checked=showYTickLines}}Show y tick lines<br/>
<button{{action "increaseQueue"}}>Increase</button>
<button{{action "decreaseQueue"}}>Decrease</button>
{{row-chart
    dimension=queueDimensions
    group=queueGroups
    colors=colors
    height=400
    width=600
    labelWidth=150
    xAxis=yAxis
    onClick=(action onClick)
    hideAxisLines=hideAxisLines
    showYTicks=showYTicks
    showYTickLines=showYTickLines
    showMaxMin=true
    chartNotAvailableBars=6
}}

{{outlet}}
