{{domainString}}

<button{{action "increaseData"}}>Increase</button>
<button{{action "decreaseData"}}>Decrease</button>

{{column-chart
    dimension=dimensions
    group=groups
    seriesData=content
    type='LAYERED'
    seriesMaxMin=2
    showMaxMin=true
    series=series
    colors=colors
    height=200
    xAxis=xAxis
    yAxis=yAxis
    showComparisonLine=true
    comparisonLine=comparisonLine
    currentInterval=currentInterval
    showCurrentIndicator=true
}}

{{line-chart
    dimension=dimensions
    group=groups
    seriesData=content
    type='LAYERED'
    seriesMaxMin=2
    showMaxMin=true
    series=series
    colors=colors
    height=200
    xAxis=xAxis
    yAxis=yAxis
    showComparisonLine=true
    comparisonLine=comparisonLine
    currentInterval=currentInterval
    showCurrentIndicator=false
}}

{{input type='checkbox' checked=donutChart}}Donut<br/>
{{input type='checkbox' checked=showTotal}}Total<br/>
{{input type='checkbox' checked=labels}}Labels<br/>
{{input type='checkbox' checked=externalLabels}}External labels<br/>
{{pie-chart
    dimension=agentDimensions
    group=agentGroups
    seriesData=agentContent
    colors=colors
    height=400
    width=800
    showTotal=showTotal
    donutChart=donutChart
    externalLabels=externalLabels
    labels=labels
}}

{{outlet}}
